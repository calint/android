	String qs="";
	public final void ax_letter(xwriter x,String[]args){
		qs=args[2];
		x.js_reload();
	}
	public void to(final xwriter w) throws Throwable{
//		req r=req.get();
//		String qs=URLDecoder.decode(r.query_s,"utf8");
//		qs=qs.replace('+',' ');
//		qs=qs.replace('/',' ');
//		qs=qs.replace('.',' ');
